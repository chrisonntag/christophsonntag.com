{
  "name": "repo",
  "version": "1.0.0",
  "private": "true",
  "main": "app.js",
  "scripts": {
    "clean": "rm -rf .cache/*",
    "build:sass": "sass styles:public/css",
    "watch:sass": "sass --watch styles:public/css",
    "build:songs": "parcel build say-it-with-songs/index.js --out-dir public/js --out-file say-it-with-songs.bundle.js --public-url ./",
    "build": "npm run clean && npm run build:sass && npm run build:songs"
  },
  "dependencies": {
    "parcel-bundler": "^1.12.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.34.0"
  }
}
